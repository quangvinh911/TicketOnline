@model TicketBox.Models.TicketBox_dbModel
@{
    ViewBag.Title = "DangKy";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<script src="~/Assets/admin/vendor/jquery/jquery.js"></script>
<link href="~/Content/css/CSSDangKy.css" rel="stylesheet" />
<link href="~/Content/css/StyleSheet1.css" rel="stylesheet" />
 <!--this was made my Agil Asadi. You are free to delete this comment line and use it as you wish-->
<div class="login-box">
    <h4>Đăng Ký</h4>
        <div class="login-body">
            <div class="form-group">
                @Html.LabelFor(model => model.TenTaiKhoan)
                <input type="text" placeholder="Tài khoản của bạn" class="form-control" />
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.MatKhau)
                <input type="text" placeholder="Mật khẩu" class="form-control" />
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.Email)
                <input type="text" placeholder="Email của bạn là gì" class="form-control" />
            </div>
            @Html.LabelFor(model => model.NgaySinh)
            <div class="form-group">
                @Html.EditorFor(model => model.NgaySinh)
                @Html.ValidationMessageFor(model => model.NgaySinh)
            </div>
            <div class="login-footer">
                <p><input type="submit" value="Xác Nhận" /></p>
                @Html.ActionLink("Đăng Nhập", "DangNhap", "DangNhap")
            </div>
        </div>
 </div>
